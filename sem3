#include <stdio.h>

int main()
{
    char a=0,b=0;
    int c;
    while((c=getchar())!='.'){
        if(c!='-'){
            if(a==0){
                a = c;
                continue;
            }
            if(b==0){
                b = c;
                if(b>a){
                    for(int z = a; z<=b; z++){
                        putchar(z);
                    }
                }
                if(a>b){
                    for(int z = a; z<=b; z--){
                        putchar(z);
                    }
                }
                a=b=0;
            }
            
        }
    }
   
    return 0;
}
//task2
#include <stdio.h>
void substruct(char s_1[256], char s_2[256]){
    
    for (int x = 0; x<256; x++){
        for (int y = 0; y<256; y++){
            if(s_1[y] == s_2[x]){
                s_1[y] = '-';
            }
        }
    }
    int counter =0;
    for(int y = 0; y<256; y++){
        if (s_1[y] >= 'a' && s_1[y]<='z'){
            putchar(s_1[y]);
        }
    }
}

int main()
{
    char s1[256];
    char s2[256]; 
    printf("Put in s1:");
    scanf("%s", &s1);
    printf("Put in s2:");
    scanf("%s", &s2);
    substruct(s1,s2);
    return 0;
}
//task3
#include <stdio.h>

int main()
{
    char s[256];
    int count = 0;
    char input;
    char sequence[]="abcdefghijklmnopqrstuvwxyz0123456789";
    int frequency[36];
    
    for (int i=0; i<36; i++){
        frequency[i] = 0;
    }
    printf("Put in s:");
    //scanf("%s", &s);
    while( (input = getchar()) != '\n'){
        s[count] = input;
        count++;
    }
    
    for(int x=0; x<count; x++){
        for(int i=0; i<36; i++){
            if(s[x]==sequence[i]){
                frequency[i]++;
            }
        }
    }
    for(int i=0; i<36; i++){
        if(frequency[i] != 0){
            printf("%c:%d\n",sequence[i],frequency[i]);
        }
    }
    return 0;
}
