#include <stdio.h>
int ToDec(char s[], int size){
    char alph[] = "0123456789abcdef";
    int output = 0;
    for(int i=0; i<size; i++){
        for( int j=0; j<16; j++){
            if(s[size-i-1]==alph[j]){
                output += j*16^i;
            }
        }
    }
    return output;
}
int main()
{
    char input[256];
    int i = 0;
    while((input[i]=getchar())!='.'){
        if(('A'<=input[i] && input[i]<='F') || ('a'<=input[i] && input[i]<='f') || ('0'<=input[i] && input[i]<='9')){
           if('A'<=input[i] && input[i]<='F'){
               input[i] = input[i] - 'A' + 'a';
           }
        } else{
            printf("Error: unknown symbol");
            return 1;
        }
        i++;
    }
    input[i] = '\0';
    
    printf("%s\n",input);
    printf("%d",ToDec(input,i));
    return 0;
}
